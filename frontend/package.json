    "yjs": "^13.6.27"
  },
  "overrides": {
    "@y/protocols": "1.0.6-1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/react-router-devtools": "^1.141.2",
    "@tanstack/router-plugin": "^1.141.2",
    "@types/node": "^24.10.1",
    "@types/phoenix": "^1.6.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
